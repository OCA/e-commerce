<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="script[last()]" position="after">
            <script
                type="text/javascript"
                src="/website_sale_attribute_filter_multiselect/static/src/lib/multiple-select/multiple-select.js"
            />
            <script
                type="text/javascript"
                src="/website_sale_attribute_filter_multiselect/static/src/lib/multiple-select/multiple-select-locale-all.js"
            />
            <script
                type="text/javascript"
                src="/website_sale_attribute_filter_multiselect/static/src/js/website_sale.js"
            />
        </xpath>
        <xpath expr="link[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/css"
                href="/website_sale_attribute_filter_multiselect/static/src/lib/multiple-select/multiple-select.css"
            />
            <link
                rel="stylesheet"
                type="text/scss"
                href="/website_sale_attribute_filter_multiselect/static/src/scss/website_sale.scss"
            />
            </xpath>
    </template>
</odoo>
