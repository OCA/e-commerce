<openerp>
<data>

<record id="view_checkout_vat_configuration" model="ir.ui.view">
    <field name="name">Checkout VAT Configuration</field>
    <field name="model">website.config.settings</field>
    <field name="inherit_id" ref="website.view_website_config_settings"/>
    <field name="arch" type="xml">
        <xpath expr="//div[@name='language_ids']/.." position="after">
            <group string="Sale Checkout VAT">
                <label for="vat_code_id"/>
                <div name="vat_code_id">
                    <div class="oe_inline">
                        <field name="vat_code_id" class="oe_inline"/>
                    </div>
                </div>
            </group>
        </xpath>
    </field>
</record>

</data>
</openerp>
