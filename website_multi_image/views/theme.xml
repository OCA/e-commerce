<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="slick_theme" inherit_id="website.assets_frontend" name="Slick Theme">
          <xpath expr="." position="inside">
            <link href="/website_multi_image/static/lib/slick/slick.css" rel="stylesheet" />
          </xpath>
        </template>
        <template id="clean_script" inherit_id="website.assets_frontend" name="Clean scripts">
          <xpath expr="/t" position="inside">
            <script type="text/javascript" src="/website_multi_image/static/lib/slick/slick.js" data-no-instant=""></script>
            <script type="text/javascript" src="/website_multi_image/static/src/js/slider.js"></script>
          </xpath>
        </template>
    </data>
</openerp>
